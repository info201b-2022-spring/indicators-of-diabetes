combo_df <- merge(census_df, small_food_df, by.x = "Name", by.y = "State.Agency.or.Indian.Tribal.Organization", all.x = TRUE)
combo_df <- merge(combo_df, region_df, by = "Name", all.x = TRUE)
filter_df <- combo_df
# Setup the UI
ui <- fluidPage(
titlePanel("SNAP Data for 2016"),
sidebarLayout(
sidebarPanel(
h5("Controls"),
sliderInput(
inputId = "poverty",
label = "Filter by max poverty percentage:",
min = 0,
max = 25,
value = 25
)
),
mainPanel(
#  plotOutput(outputId = "scatter", click = "plot_click"),
plotOutput(outputId = "scatter", brush = "plot_brush"),
tableOutput(outputId = "data"),
plotlyOutput(outputId = "plotlyScatter")
)
)
)
# Server logic goes here
server <- function(input, output){
output$scatter <- renderPlot({
filter_df <- filter(combo_df, Poverty.Percent..All.Ages <= input$poverty)
ggplot(data = filter_df, aes(x = Poverty.Percent..All.Ages, y = yr_mean)) +
geom_point(aes(col = Region.Name))
})
output$data <- renderTable({
#  nearPoints(filter_df, input$plot_click, xvar = "Poverty.Percent..All.Ages", yvar = "yr_mean")
brushedPoints(filter_df, input$plot_brush)
})
output$plotlyScatter <- renderPlotly({
plot_ly(data = combo_df, x = ~Poverty.Percent..All.Ages, y = ~yr_mean, color = ~Region.Name, ~type = "scatter")
})
}
# This is the function that makes the shiny app
shinyApp(ui = ui, server = server)
runApp('~/Desktop/INFO 201/lecture/shiny demos/lect17')
